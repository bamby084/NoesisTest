    <Style TargetType="Button" x:Key="DefaultButtonStyle">
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Background" Value="{StaticResource WhiteColor}"/>
        <Setter Property="Foreground" Value="{StaticResource SecondaryColor}"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border CornerRadius="12" BorderThickness="{TemplateBinding BorderThickness}" BorderBrush="{TemplateBinding BorderBrush}"
                            Background="{TemplateBinding Background}" TextBlock.Foreground="{TemplateBinding Foreground}"
                            x:Name="Border">
                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <VisualState x:Name="Normal"/>
                                <VisualState x:Name="MouseOver">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName ="Border" 
                                                        Storyboard.TargetProperty="(Button.Background).(SolidColorBrush.Color)"
                                                        Duration="0:0:0.05"
                                                        To="{StaticResource Color.TKM.SecondaryColorTint4}"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <ColorAnimation Storyboard.TargetName ="Border" 
                                                        Storyboard.TargetProperty="(Button.Background).(SolidColorBrush.Color)"
                                                        Duration="0:0:0.05"
                                                        To="{StaticResource Color.TKM.GrayscaleColorTint9}"/>
                                    </Storyboard>
                                </VisualState>
                                <VisualState x:Name="Disabled">

                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>

                        <ContentPresenter Margin="2" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <DataTemplate x:Key="MenuItemNormalTemplate" DataType="{x:Type local:MenuItem}">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="24 16">
            <local:Icon Symbol="{Binding Icon}" Color="{StaticResource MainColorTint2}" x:Name="Icon" VerticalAlignment="Center"/>
            <TextBlock Text="{Binding Title}" Margin="12 0 0 0" x:Name="Title"
                   FontFamily="{StaticResource urbanistFont}" FontSize="16"
                   Foreground="{StaticResource MainColorTint2}" VerticalAlignment="Center"/>
        </StackPanel>
    </DataTemplate>

    <DataTemplate x:Key="MenuItemSelectedTemplate" DataType="{x:Type local:MenuItem}">
        <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Margin="24 16">
            <local:Icon Symbol="{Binding Icon}" Color="{StaticResource SecondaryColor}" x:Name="Icon" VerticalAlignment="Center"/>
            <TextBlock Text="{Binding Title}" Margin="12 0 0 0" x:Name="Title"
                   FontFamily="{StaticResource urbanistFont}" FontSize="16"
                   Foreground="{StaticResource SecondaryColor}" VerticalAlignment="Center"/>
        </StackPanel>
    </DataTemplate>

    <DataTemplate x:Key="MenuGroupItemTemplate" DataType="{x:Type local:MenuItem}">
        <TextBlock Text="{Binding Title}" FontFamily="{StaticResource urbanistFont}" FontSize="14"
           Foreground="{StaticResource MainColorTint3}" Margin="2"/>
    </DataTemplate>
    
    <Style TargetType="ListBoxItem" x:Key="MenuItemContainerStyle">
        <Setter Property="SnapsToDevicePixels" Value="True"/>
        <Setter Property="OverridesDefaultStyle" Value="True"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Margin" Value="1"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ListBoxItem">
                    <Border x:Name="Border" SnapsToDevicePixels="True" Background="Transparent" CornerRadius="16">
                        <ContentPresenter/>
                    </Border>
                    
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource SecondaryColorTint4}"/>
                        </Trigger>
                        <Trigger Property="IsSelected" Value="True">
                            <Setter TargetName="Border" Property="Background" Value="{StaticResource SecondaryColorTint4}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
        <Style.Triggers>
            <DataTrigger Binding="{Binding MenuItemType}" Value="{x:Static local:MenuItemType.GroupItem}">
                <Setter Property="IsHitTestVisible" Value="False"/>
                <Setter Property="Margin" Value="0 5"/>
                <Setter Property="ContentTemplate" Value="{StaticResource MenuGroupItemTemplate}"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding MenuItemType}" Value="{x:Static local:MenuItemType.Normal}">
                <Setter Property="ContentTemplate" Value="{StaticResource MenuItemNormalTemplate}"/>
            </DataTrigger>
            <Trigger Property="IsSelected" Value="True">
                <Setter Property="ContentTemplate" Value="{StaticResource MenuItemSelectedTemplate}"/>
            </Trigger>
        </Style.Triggers>
       
    </Style>
</ResourceDictionary>